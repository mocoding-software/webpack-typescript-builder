{
  "name": "webpack-typescript-builder",
  "version": "3.1.0",
  "description": "Webpack config builder for typescript, styles (css and sass), fonts and images.",
  "main": "lib/index.js",
  "typings": "index.d.js",
  "bin": {
    "wtb": "lib/cli/index.js"
  },
  "scripts": {
    "start": "ts-node src/cli build ./sample",
    "test": "ts-node src/cli",
    "build": "tslint --project . && tsc",
    "prerelease": "npm run build",
    "wtb": "ts-node src/cli",
    "wtb:config": "ts-node src/cli config ./examples/basic",
    "wtb:build": "ts-node src/cli build ./examples/basic",
    "wtb:serve": "ts-node src/cli serve ./examples/basic",
    "wtb:build-redux": "ts-node src/cli build ./examples/redux",
    "wtb:serve-redux": "ts-node src/cli serve ./examples/redux"
  },
  "keywords": [
    "webpack",
    "typescript",
    "saas",
    "fonts",
    "image",
    "builder",
    "utils"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mocoding-software/webpack-typescript-builder.git"
  },
  "bugs": {
    "url": "https://github.com/mocoding-software/webpack-typescript-builder/issues",
    "email": "social@mocoding.com"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/mocoding-software/webpack-typescript-builder#readme",
  "author": "MOCODING, LLC",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/extract-text-webpack-plugin": "^3.0.4",
    "@types/mini-css-extract-plugin": "^0.8.0",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet-async": "^1.0.3",
    "@types/react-redux": "^7.1.5",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.7",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-middleware": "^2.0.3",
    "@types/webpack-env": "^1.14.1",
    "@types/webpack-manifest-plugin": "^2.1.0",
    "cache-loader": "4.1.0",
    "commander": "^4.0.1",
    "css-hot-loader": "1.4.4",
    "css-loader": "3.3.2",
    "express": "^4.17.1",
    "file-loader": "5.0.2",
    "fork-ts-checker-webpack-plugin": "3.1.1",
    "ignore-loader": "0.1.2",
    "import-glob": "1.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "4.13.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.4",
    "react-hot-loader": "^4.12.18",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.1",
    "thread-loader": "2.1.3",
    "ts-loader": "6.2.1",
    "ts-node": "^8.5.4",
    "tslint": "5.20.1",
    "tslint-loader": "3.5.4",
    "tslint-react": "4.1.0",
    "typescript": "3.7.3",
    "url-loader": "3.0.0",
    "webpack": "4.41.2",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.0",
    "webpack-manifest-plugin": "^2.2.0"
  }
